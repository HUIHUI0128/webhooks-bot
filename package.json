{
  "name": "docking-bot",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "pm2 start app.js --name='docking-bot' --watch",
    "stop": "pm2 stop docking-bot",
    "build-mac": "pkg -t macos cbs.js -o dist/cbs-mac",
    "build-win": "pkg -t win cbs.js -o dist/cbs-win",
    "build-linux": "pkg -t linux cbs.js -o dist/cbs-linux",
    "build-all": "pkg -t macos,linux,win cbs.js -o dist/cbs",
    "clear": "ssh feserver 'rm -rf ~/nodejs-apps/common-base-robots'",
    "pub-dist": "scp -r ./dist afe@feserver:~/nodejs-apps/common-base-robots",
    "pub-bash": "scp -r ./bash afe@feserver:~/nodejs-apps/common-base-robots",
    "clear-dist": "rm -rf dist",
    "restart": "ssh feserver 'pm2 restart cbs-linux'",
    "deploy": "yarn clear-dist && yarn build-linux && yarn clear && yarn pub-dist && yarn restart"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-ws": "^5.0.2",
    "http-proxy-middleware": "^2.0.6",
    "js-base64": "^3.7.2",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "node-schedule": "^2.0.0",
    "pkg": "^5.8.0",
    "pm2": "^5.1.0"
  }
}